<!DOCTYPE html>
<html>
<head>
  <title>Jual Online Berkualitas | Coastline-Goods</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta property="og:title" content="Jual Beli Online Berkualitas - Coastline-Goods" />
  <meta property="og:site_name" content="coastline-Goods.id" />
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag "products" %>
  <%= favicon_link_tag 'green.png' %>
  
</head>
<body>

<%= render 'layouts/header' %>
<div class="container">
 <% flash.each do |name, msg| %>
 	<% if msg.is_a?(String) %>
 		<div class="alert alert-<%= name == :notice ? "success" : "danger" %> alert-dismissable">
 			<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
 			<%= content_tag :div, msg, :id => "flash_#{name}" %>
 		</div>
 	<% end %>
 <% end %>
</div>
</div>
<%= yield %>
	

<%= render 'layouts/footer' %>


</body>
</html>
